<% if @user.errors.any? %>
  <div id="errors_explanation">
    <div class="alert alert-error">
      The form has <%= pluralize(@user.errors.count, "error")%>.
    </div>
    <ul>
    <% @user.errors.full_messages.each do |msg| %>
      <% if not msg.include?("digest") %>
        <li><%= msg %>
      <% end %>
    <% end %>
    </ul>
  </div>
<% end %>
